{
  "name": "olcs/olcs-xmltools",
  "description": "Xml tools for olcs",
  "repositories": [
    {
      "packagist": false
    },
    {
      "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
      "type": "composer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "zendframework/zendxml": "~1.0.2",
    "zendframework/zend-filter": "~2.3.7",
    "zendframework/zend-servicemanager": "~2.3.7",
    "zendframework/zend-validator": "~2.3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.0",
    "mikey179/vfsStream": "~1.4.0",
    "mockery/mockery": "~0.9.3"
  },
  "config": {
    "secure-http" : false
  },
  "autoload" : {
    "psr-4" : {
      "Olcs\\XmlTools\\": "src//"
    }
  }

}
